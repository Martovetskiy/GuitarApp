<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MusicPlayer.LogOutPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" Orientation="Vertical">
            <Label x:Name="MyUserName" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand"/>
            <Button x:Name="Logout" Text="Выйти" BackgroundColor="Black" TextColor="White" Clicked="Logout_Clicked"/>
            <Button x:Name="AdminPanel" IsEnabled="False" Text="Админ Панель" BackgroundColor="Black" TextColor="White" Clicked="AdminPanel_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>